<%= form_for @listing do |f| %>
  <div>
    <%= f.label :title %>
    <%= f.text_field :title %>
  </div>
  <div>
    <%= f.label :description %>
    <%= f.text_area :description %>
  </div>
  <div>
    <%= f.label :max_guests %>
    <%= f.number_field :max_guests %>
  </div>
  <div>
    <%= f.label :price %>
    <%= f.number_field :description %>
  </div>
  <%= f.submit "Submit" %>
<% end %>
